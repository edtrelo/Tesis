  author = {Storvik, Geir and Diz-Lois Palomares, Alfonso and Engebretsen, Solveig and Rø, Gunnar Øyvind Isaksson and Engø-Monsen, Kenth and Kristoffersen, Anja Bråthen and De Blasio, Birgitte Freiesleben and Frigessi, Arnoldo},
