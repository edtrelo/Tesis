@article{osthusForecastingSeasonalInfluenza2017,
  title = {Forecasting Seasonal Influenza with a State-Space {{SIR}} Model},
  author = {Osthus, Dave and Hickmann, Kyle S. and Caragea, Petruţa C. and Higdon, Dave and Del Valle, Sara Y.},
  date = {2017-03-01},
