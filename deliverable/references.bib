@article{ashokkumarPersistenceCoronavirusSurface2023,
  title = {Persistence of {{Coronavirus}} on {{Surface Materials}} and {{Its Control Measures Using Nonthermal Plasma}} and {{Other Agents}}},
  author = {Ashokkumar, Sekar and Kaushik, Nagendra Kumar and Han, Ihn and Uhm, Han Sup and Park, Jang Sick and Cho, Gyu Seong and Oh, Young-Jei and Shin, Yung Oh and Choi, Eun Ha},
