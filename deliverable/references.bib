@article{arnoldWhenArtificialParameter2023,
  title = {When {{Artificial Parameter Evolution Gets Real}}: {{Particle Filtering}} for {{Time-Varying Parameter Estimation}} in {{Deterministic Dynamical Systems}}},
  shorttitle = {When {{Artificial Parameter Evolution Gets Real}}},
  author = {Arnold, Andrea},
  date = {2023-01-01},
  journaltitle = {Inverse Problems},
  shortjournal = {Inverse Problems},
  volume = {39},
  number = {1},
  eprint = {2204.00074},
